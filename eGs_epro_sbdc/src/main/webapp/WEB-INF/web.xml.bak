<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>EUNWOO APPLICATION</display-name>
  <filter>
    <filter-name>encodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>utf-8</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>encodingFilter</filter-name>
    <url-pattern>*.do</url-pattern>
    <url-pattern>*.dx</url-pattern>
  </filter-mapping>
  <filter>
    <filter-name>XSS</filter-name>
    <filter-class>com.eunwoosoft.frwk.prl.xss.FwkXssFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>XSS</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
  </listener>
  <context-param>
    <param-name>contextConfigLocation</param-name>
    <param-value>        	
        	classpath:/config/spring/context-ew.xml
        </param-value>
  </context-param>
  <servlet>
    <servlet-name>UbiServer</servlet-name>
    <servlet-class>com.ubireport.server.UbiServer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UbiServer</servlet-name>
    <url-pattern>/UbiServer</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>actionRaon</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/config/springmvc/ew-raonservlet.xml</param-value>
    </init-param>
    <init-param>
      <param-name>spring.profiles.active</param-name>
      <param-value>production</param-value>
    </init-param>
    <load-on-startup>2</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>actionRaon</servlet-name>
    <url-pattern>*.dx</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>action</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/config/springmvc/ew-servlet.xml</param-value>
    </init-param>
    <init-param>
      <param-name>spring.profiles.active</param-name>
      <param-value>production</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>action</servlet-name>
    <url-pattern>*.do</url-pattern>
    <url-pattern>*.json</url-pattern>
    <url-pattern>*.iframe</url-pattern>
  </servlet-mapping>
  <session-config>
    <session-timeout>360</session-timeout>
  </session-config>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/erro/error.do</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.Exception</exception-type>
    <location>/erro/error.do</location>
  </error-page>
  <error-page>
    <error-code>400</error-code>
    <location>/erro/error400.do</location>
  </error-page>
  <error-page>
    <error-code>403</error-code>
    <location>/erro/error403.do</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/erro/error404.do</location>
  </error-page>
  <error-page>
    <error-code>405</error-code>
    <location>/erro/error405.do</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/erro/error500.do</location>
  </error-page>
  <error-page>
    <error-code>503</error-code>
    <location>/erro/error500.do</location>
  </error-page>
</web-app>